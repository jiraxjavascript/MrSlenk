const Discord = require('discord.js')

module.exports = {
	name: 'usmev',
	description: 'usmeje se',
	cooldown: 4,
	async execute(message, args) {
        const fetch = require("node-fetch");
      	var json = await (await fetch(":smiley:")).json();
      	if(!json.url) return;
      
      	var embed = new Discord.MessageEmbed()
        	.setImage(json.url)
        	.setTitle(json.title)
        	.setFooter('pekne se usmiva co | Bota vytvoril Slenky#0001')
        	.setURL('http://memesare.fun')
     	message.channel.send(embed)
    },
};
